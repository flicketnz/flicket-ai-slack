{
  "name": "app",
  "private": true,
  "version": "1.0.0",
  "description": "Flicket Agent Platform as a NestJS App",
  "license": "INTENRAL",
  "scripts": {
    "build": "nest build",
    "deploy:prod": "./scripts/slack-deploy-prod.bash",
    "start": "nest start",
    "start:dev": "slack run --app $(jq -r '.custom_managed_local_app.app_id // empty' .slack/custom-managed-apps.json)",
    "start:prod": "node dist/main",
    "lint": "eslint src/**/*",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest --coverage"
  },
  "engines": {
    "npm": ">=11.0.0",
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.863.0",
    "@langchain/community": "^0.3.49",
    "@langchain/core": "^0.3.66",
    "@langchain/langgraph": "^0.4.3",
    "@langchain/langgraph-checkpoint": "^0.1.0",
    "@langchain/openai": "^0.6.3",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "11.1.5",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "11.1.5",
    "@nestjs/terminus": "11.0.0",
    "@slack/bolt": "^4.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "joi": "^18.0.0",
    "langchain": "^0.3.30",
    "nestjs-dynamoose": "^0.6.0",
    "nestjs-slack-bolt": "^1.4.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@langchain/langgraph-checkpoint-validation": "^0.1.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@slack/cli-hooks": "^1.2.1",
    "@types/joi": "^17.2.2",
    "@vitest/ui": "^3.2.4",
    "dynamoose-logger": "^4.0.4",
    "testcontainers": "^11.5.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "nestjs-slack-bolt": {
      "@slack/bolt": "$@slack/bolt"
    },
    "@langchain/langgraph-checkpoint-validation": {
      "@langchain/langgraph-checkpoint": "$@langchain/langgraph-checkpoint"
    }
  }
}
